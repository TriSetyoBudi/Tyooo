#include <iostream>
using namespace std;

int Kecepatan (int s, int t){
    int kecepatan;
    kecepatan = s / t;
    return kecepatan;
}
int main()
{
    int a,b;
    cout << "Masukkan Jarak : "; cin >> a;
    cout << "Masukkan Waktu : "; cin >> b;

    cout << "Kecepatan rata-rata: " << Kecepatan (a,b);
    return 0;
}
